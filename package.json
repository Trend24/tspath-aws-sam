{
  "name": "tspath-aws-sam",
  "version": "2.0.0",
  "description": "TypeScript Path Igniter resolves @paths specified in tsconfig",
  "scripts": {
    "build": "tsc",
    "tspath": "node ./dist/index.js",
    "watch": "tsc -w"
  },
  "bin": {
    "tspath": "./dist/index.js"
  },
  "keywords": [
    "relative",
    "path",
    "typescript",
    "path",
    "resolve",
    "alias"
  ],
  "author": "Bart van Sleeuwen <bart.vansleeuwen@pidz.nl>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pidz-development/tspath-aws-sam.git"
  },
  "license": "LGPL-2",
  "dependencies": {
    "@types/node": "^8.0.25",
    "chalk": "^2.4.2",
    "prompt-confirm": "^2.0.4",
    "typescript": "^2.4.2",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.2",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0"
  }
}
